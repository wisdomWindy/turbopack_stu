{
  "$schema": "https://turbo.build/schema.json",
  
  "pipeline":{
    "build":{
      "dependsOn": ["^build"],
      "inputs":["src/index.js"],
      "outputs":["/dist/**"]
    },
    "dev":{
     
      "inputs": [
        "src/index.js"
      ],
      "outputs": [
        "dist/**"
      ],
      "env":["VUE_APP_"]
    }
  }
}